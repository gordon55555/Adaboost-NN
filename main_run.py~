import getopt
import sys

try:
    opts, args = getopt.getopt(sys.argv[1:], "h:o:", ["help", "output="])
    print opts,args
except getopt.GetoptError:
    print "error"
for x,y in opts:
    if x in ("-h","--help"):
        print "help"
    if x in ("-o","--output"):
        print "output"
        sys.exit()
